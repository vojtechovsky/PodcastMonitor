@model PodcastMonitor.Web.Api.Models.Feed.FeedListViewModel
 
@using (Html.BeginForm(null, null, FormMethod.Post))
{
    @Html.ValidationSummary(true)
    <div class="area">
        <fieldset>
            <legend>Feeds</legend>ssss
            @Html.EditorForModel()
        </fieldset>
        <input type="submit" value="Save" />            
  
